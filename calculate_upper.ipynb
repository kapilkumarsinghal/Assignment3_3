   "source": [
    "def Count_Upper(s):\n",
    "    cu=0\n",
    "    cl=0\n",
    "    for i in s:\n",
    "        if i.isupper():\n",
    "            cu =cu+1\n",
    "        if i.islower():\n",
    "            cl=cl+1\n",
    "    print(\"No. of Upper case characters :\",cu)\n",
    "    print(\"No. of Lower case Characters :\",cl)\n",
    "s=input()\n",
    "Count_Upper(s)"
   ]
